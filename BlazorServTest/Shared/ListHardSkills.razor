<div class="table-responsive">
    <table class="table table-striped">
        <thead class="thead-light">
            <tr>
                <th>Skill</th>
                <th>Year</th>
                <th>My Level</th>
            </tr>
        </thead>
        <tbody>
           <GenericList ListItems="MySkills" Context="skil">
               <ItemTemplate>
                   <Skill HSkill="skil" isTable="true"/>
               </ItemTemplate>
           </GenericList>
        </tbody>
    </table>
</div>

@code {
    [Parameter]
    public List<HardSkills> MySkills { get; set; }

}
